{
    "name": "stefandoorn/google-tag-manager-enhanced-ecommerce-plugin",
    "type": "sylius-plugin",
    "description": "Google Tag Manager Enhanced Ecommerce plugin for Sylius.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "sylius/sylius": "^1.0|^1.0@rc|^1.0@beta|dev-master",
        "stefandoorn/google-tag-manager-plugin": "~0.1|dev-master"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friends-of-behat/context-service-extension": "^0.3",
        "friends-of-behat/cross-container-extension": "^0.2",
        "friends-of-behat/performance-extension": "^1.0",
        "friends-of-behat/service-container-extension": "^0.3",
        "friends-of-behat/symfony-extension": "^0.2.1",
        "friends-of-behat/variadic-extension": "^0.1",
        "lakion/mink-debug-extension": "^1.2.3",
        "se/selenium-server-standalone": "^2.52",
        "kint-php/kint": "^2.2",
        "matthiasnoback/symfony-dependency-injection-test": "^1.1",
        "phpspec/phpspec": "^3.2",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "GtmEnhancedEcommercePlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\GtmEnhancedEcommercePlugin\\": "tests/"
        }
    }
}
